'.source.swift':
  'Package.swift':
    'prefix': 'spm'
    'body': '''
    import PackageDescription

    let package = Package(
      name: "${1:PackageName}",
      targets: [$2],
      dependencies: [$3]
    )
    '''

  'Package Dependencies':
    'prefix': 'package'
    'body': '.Package(url: "https://github.com/${1:account}/${2:package}", versions: Version(.min, .min, .min) ..< Version(.max, .max, .max))'
