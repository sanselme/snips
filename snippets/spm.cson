'.source.swift':
  'Package.swift':
    'prefix': 'spm'
    'body': '''
    import PackageDescription


    let urls = [
      "https://${6:github.com}/${7:account}/${8:package}"
    ];

    let package = Package(
        name: "${1:PackageName}",
        dependencies: urls.map { .Package(url: \\$0, versions: Version(.min, .min, .min) ..< Version(.max, .max, .max)) }
    )
    '''

  'Package Dependencies':
    'prefix': 'package'
    'body': '.Package(url: "https://${1:github.com}/${2:account}/${3:package}", versions: Version(.min, .min, .min) ..< Version(.max, .max, .max))'

  'Package URLs':
    'prefix': 'purl'
    'body': '''
    "https://${1:github.com}/${2:account}/${3:package}"
    '''
